    . ~/.dotfiles/dotfiles-atom
fi

if [ -f ~/.dotfiles/dotfiles-jenv ]; then
    . ~/.dotfiles/dotfiles-jenv
fi

if [ -f ~/.dotfiles/dotfiles-ivy-cli ]; then
    . ~/.dotfiles/dotfiles-ivy-cli
fi

if [ -f ~/.dotfiles/dotfiles-k8s ]; then
    . ~/.dotfiles/dotfiles-k8s
fi

if [ -f ~/.dotfiles/dotfiles-profile ]; then
    . ~/.dotfiles/dotfiles-profile
fi

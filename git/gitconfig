[user]
	name = Jason Wall
	email = javajo@gmail.com
[core]
	excludesfile = /Users/jason/.gitignore_global
[push]
	default = simple
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[alias]
    ap = add -p
    co = checkout
    cob = checkout -b
    cop = checkout -p
    cm = commit -m
    ce = commit -F .git/COMMIT_EDITMSG
    dc = diff --cached
    rp = reset -p
    st = status
    un = log --oneline --graph --decorate @{u}..
    un-synced = log --oneline --graph --decorate @{u}..
